Xft.dpi: 96
Xft.antialias: true
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight


! Colors
! --------------------------------------------------------------------
!TomorrowNight
#define t_background        #1d1f21
#define t_current_line      #282a2e
#define t_selection      	#373b41
#define t_foreground      	#c5c8c6
#define t_comment	     	#969896
#define t_red		     	#cc6666
#define t_orange	     	#de935f
#define t_yellow        	#f0c674
#define t_green	        	#b5bd68
#define t_aqua	        	#8abeb7
#define t_blue	        	#81a2be
#define t_purple        	#b294bb

! Rofi Settings
! --------------------------------------------------------------------
rofi.color-enabled:   true
rofi.color-window:    t_background, t_aqua
rofi.color-normal:    t_background, t_foreground, t_background, t_aqua, t_background
rofi.color-active:    t_background, t_foreground, t_background, t_aqua, t_background
rofi.color-urgent:    t_background, t_foreground, t_background, t_red,  t_background
rofi.bw:              2
rofi.hide-scrollbar:  true
rofi.separator-style: none
rofi.lines:           5
rofi.padding:         30
rofi.dpi:	          96

*.foreground:   t_foreground
*.background:   t_background
*.cursorColor:  #aeafad

! Black / Grey
*.color0:       #000000
*.color8:       #666666

! Red / Bright Red
*.color1:       t_red
*.color9:       #FF3334

! Green + Bright Green
*.color2:       t_green
*.color10:      #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
*.color3:       t_orange
*.color11:      t_yellow

! Blue + Bright Blue
*.color4:       t_blue
*.color12:      t_blue

! Magenta (Purple) + Bright Magenta
*.color5:       t_purple
*.color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
*.color6:       t_aqua
*.color14:      #54ced6

! Light Grey (Selection) + White (Current Line)
*.color7:       t_selection
*.color15:      t_current_line
